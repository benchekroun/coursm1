algues <- read.table("algae.txt", header=TRUE)
s<- summary(algues)
attach(algues)
yule <- function(X){
	if(is.numeric(X)){
		qs <- quantile(X,na.rm=T,names=FALSE)
		Q1 <- qs[2]
		med <- qs[3]
		Q3 <- qs[4]
		result <- ((Q3-med)-(med-Q1))/((Q3-med)+(med-Q1))
 		
		return(result)
	} else {
		return (NA)
	}
} 
